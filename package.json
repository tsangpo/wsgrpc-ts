{
  "name": "wsgrpc",
  "version": "3.2.3",
  "description": "A RPC code generator and runtime. Codes of client side and server side are generated from a grpc proto file.",
  "author": "tsangpo <tsangpozheng@gmail.com> (https://github.com/tsangpo)",
  "repository": "github:tsangpo/wsgrpc-ts",
  "license": "ISC",
  "bin": {
    "wsgrpc": "dist/bin.cjs"
  },
  "type": "module",
  "main": "dist/lib.cjs",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "require": "./dist/lib.cjs",
      "import": "./dist/lib.mjs",
      "types": "./dist/lib.d.js"
    },
    "./node": {
      "require": "./node.cjs",
      "import": "./node.mjs",
      "types": "./node.d.js"
    }
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.29.5",
    "@types/node": "^18.7.13",
    "@types/ws": "^8.5.3",
    "esbuild": "^0.15.5",
    "protobufjs": "^6.11.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "node-fetch": "^3.2.10",
    "ws": "^8.8.1"
  }
}
